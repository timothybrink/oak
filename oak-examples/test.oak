"
This is just a super basic bunch of working Oak syntax.
"

(def .'function' /test .{
  (print (+ (test 0) (test 1)))

  [(test 2)]
})


(function ["Hello " "World," "from Oak.Rust!"])
(print (^ 0))

(if (= (type ^) 'function') .{
  (print 'is a function')
  true
} .{
  (print 'is not a function')
  false
})